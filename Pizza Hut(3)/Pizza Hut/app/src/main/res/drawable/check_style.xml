<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 选中状态 -->
    <item android:drawable="@mipmap/check_selected" android:state_checked="true"/>
    <!-- 不选中状态 -->
    <item android:drawable="@drawable/selected" android:state_checked="false"/>
    <!-- 按下状态 -->
    <item android:drawable="@drawable/selected" android:state_pressed="true"/>
</selector>